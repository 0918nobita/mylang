{
  "private": true,
  "name": "vscode-mylang-mode",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.63.2"
  },
  "main": "./build/bundle.js",
  "scripts": {
    "build": "esbuild --platform=node --bundle --minify --external:vscode src/main.ts --outfile=build/bundle.js"
  },
  "dependencies": {
    "vscode-languageclient": "7.0.0"
  },
  "devDependencies": {
    "@types/node": "17.0.14",
    "@types/vscode": "1.64.0",
    "esbuild": "0.14.20",
    "eslint": "8.8.0",
    "typescript": "4.5.5"
  },
  "activationEvents": [
    "onLanguage:mylang"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mylang",
        "extensions": [
          ".mylang"
        ]
      }
    ]
  }
}
